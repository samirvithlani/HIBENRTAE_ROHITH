package java8;

interface Changes {

	public String Data(String name, String lastname);
}

public class LamdaDemo3 implements Changes {

	@Override
	public String Data(String name, String lastname) {
		// TODO Auto-generated method stub
		return name.concat(lastname);
	}
	
	public static void main(String[] args) {
	
		LamdaDemo3 l = new LamdaDemo3();
		System.out.println(l.Data("raj", "ram"));
		
	}
	


}
